year <- sub("-.*", "", meta$Date)
note <- sprintf("{R} package version %s", meta$Version)

citEntry(
  entry = "Misc",
  title = "{dequer}: Stacks, Queues, and Deques for R",
  author = person("Drew", "Schmidt"),
  year = year,
  note = note,
  url = "https://cran.r-project.org/package=dequer",
  textVersion = NULL,
  key = "dequerpackage"
)
